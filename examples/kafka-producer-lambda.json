{
  "name": "kafkaProducer",
  "description": "This lambda generates a stream of sample data and stores it in a kafka queue.  Code is available at: https://github.com/GalacticFog/lambda-examples/tree/master/scala_lambda/kafka_producer",
  "properties": {
    "apiendpoints": [],
    "package_url": "https://s3.amazonaws.com/gfi.lambdas/kafka-producer-assembly-0.1.jar",
    "public": true,
    "compressed": false,
    "cpus": 0.1,
    "code_type": "package",
    "headers": {
      "Accept": "text/plain"
    },
    "periodic_info": {},
    "timeout": 30,
    "handler": "com.galacticfog.Producer;produce",
    "runtime": "java;scala",
    "env": {
      "KAFKA_TOPIC": "avro-input",
      "KAFKA_BROKERS": "REPLACE ME:9092"
    },
    "memory": 256
  }
}
