{
  "name": "peek-kafka",
  "description": "Peek Kafka Lambda",
  "properties": {
    "package_url": "https://s3.amazonaws.com/gfi.lambdas/peek-kafka-assembly-0.1.jar",
    "public": true,
    "compressed": false,
    "cpus": 1,
    "code_type": "package",
    "headers": {
      "Accept": "text/plain"
    },
    "periodic_info": {},
    "timeout": 30,
    "handler": "com.galacticfog.Peeker;peek",
    "runtime": "java;scala",
    "env": {
      "KAFKA_BROKERS": "REPLACE ME"
    },
    "memory": 256
  }
}
